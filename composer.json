{
	"name": "innologi/typo3-appointments",
	"type": "typo3-cms-extension",
	"description": "Appointment scheduler allows FE users to schedule / list / manage appointments. Agenda's are created in BE, including user-defined appointment types, conditions, registration forms, and more.",
	"license": "GPL-2.0-or-later",
	"authors": [{
			"name": "Frenck Lutke",
			"email": "typo3@innologi.nl",
			"homepage": "http://www.innologi.nl",
			"role": "Developer"
		}
	],
	"require": {
		"php": ">=8.2",
		"typo3/cms-core": "^12.4",
		"typo3/cms-felogin": "^12.4",
		"typo3/cms-scheduler": "^12.4",
		"innologi/typo3-asset-provider": "~1.0.0",
		"bmack/modern-template-building": "dev-master#88864b6e82fa78c015a8ea0d18be490782a4f096",
		"friendsoftypo3/tt-address": "^9.0.0"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "appointments"
		}
	},
	"autoload": {
		"psr-4": {
			"Innologi\\Appointments\\": "Classes/"
		}
	},
	"replace": {
		"typo3-ter/appointments": "self.version"
	}
}